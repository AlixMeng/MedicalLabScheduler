<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="ButtonGradientBrush" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FFCEE0F5" Offset="0"/>
        <GradientStop Color="#FFDDEEFF" Offset="0.041666666666666664"/>
        <GradientStop Color="#FFE5F0FD" Offset="0.5"/>
        <GradientStop Color="#FFAECBEF" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ButtonGradientBrush2" EndPoint="1.204,0.5" StartPoint="0.056,0.5">
        <GradientStop Color="#FFDDEEFF" Offset="0"/>
        <GradientStop Color="#FFBFDBFF" Offset="0.5"/>
        <GradientStop Color="#FFAECBEF" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BorderGradientBrush" EndPoint="1.204,0.5" StartPoint="0.056,0.5">
        <GradientStop Color="#FFFFFFFF" Offset="0" />
        <GradientStop Color="#FFD4D7DB" Offset="0.5" />
        <GradientStop Color="#FFD4D7DB" Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="LightBlueStatus" Color="#FFBFDBFF"/>
    <SolidColorBrush x:Key="LightBlueButton" Color="#FFAECBEF"/>

    <SolidColorBrush x:Key="BackgroundBrush" Color="#FFCEE0F5"/>
</ResourceDictionary>